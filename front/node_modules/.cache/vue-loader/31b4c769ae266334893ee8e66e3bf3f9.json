e\">\n        <h2>{{ name }}</h2>\n        <p>{{ description }}</p>\n      </div>\n      <div class=\"timer\">\n        <div class=\"btn\">\n            <div @click=\"view()\" class=\"timer-btn\">ПРОСМОТРЕТЬ</div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Viewer from 'viewerjs'\nimport 'viewerjs/dist/viewer.css'\n\nexport default {\n  name: \"NftBox\",\n  props: {\n    name: String,\n    description: String,\n    image: Number\n  },\n  data() {\n    return {\n        play: false,\n    }\n  },\n  mounted() {\n    const viewer = new Viewer(document.getElementById('image'+this.image), {\n      inline: false,\n      toolbar: false,\n      navbar: false,\n      title: false,\n      loop: false,\n      viewed() {\n        viewer.zoomTo(0.5);\n      },\n    });\n  },\n\n  methods: {\n    view() {\n      document.getElementById('image'+this.image).click();\n      if (this.play)\n        return;\n      this.play = true;\n      const audio = document.getElementById('music');\n      audio.play();\n    }\n  }\n}\n</script>\n\n<style>\n\n.nft_box {\n  margin-top: 100px;\n  width: 23%;\n  min-height: 530px;\n  padding: 30px 30px 20px;\n  display: flex;\n  flex-direction: column;\n  background: rgba(43, 43, 43, 1);\n  color: white;\n}\n.collection_image {\n  width: 65%;\n  align-self: center;\n}\n.collection_image img {\n  width: 100%;\n}\n.title {\n  width: fit-content;\n  align-self: start;\n  margin-top: 30px;\n}\n.title h2 {\n  color: white;\n  font-size: 26px;\n  font-family: Noto Sans,serif;\n  line-height: 49px;\n  margin-bottom: 8px;\n}\n.title p {\n  text-align: left;\n  font-size: 18px;\n  line-height: 25px;\n  margin-bottom: 40px;\n}\n.timer {\n  align-self: center;\n  display: flex;\n  flex-direction: column;\n}\n\n.timer p {\n  font-weight: bold;\n  margin-bottom: 26px;\n}\n.timer div {\n  width: 200px;\n  align-self: center;\n  padding-top: 10px;\n  padding-bottom: 10px;\n  border-radius: 11px;\n  background-color: #00D1FF;\n  color: black;\n  font-size: 14px;\n  font-weight: bold;\n}\n.timer .btn div {\n  cursor: not-allowed;\n}\n.timer-btn {\n  cursor: pointer !important;\n}\n.author {\n  position: relative;\n  background-color: #00D1FF;\n  color: black;\n  font-weight: bold;\n  font-size: 20px;\n  width: fit-content;\n  align-self: end;\n  padding-left: 30px;\n  padding-right: 30px;\n  padding-top: 15px;\n  padding-bottom: 15px;\n  left: 30px;\n  bottom: 60px;\n}\n</style>\n\n<style scoped>\n\n</style>\n"]}]}